task.spawn(function()
	Main.BackgroundTransparency = 1
	Title.TextTransparency = 1
	TopBar.BackgroundTransparency = 1
	BottomAccent.BackgroundTransparency = 1

	for i = 1, 20 do
		Main.BackgroundTransparency = 0.1 - (i/20 * 0.1)
		Title.TextTransparency = 1 - (i/20)
		TopBar.BackgroundTransparency = 0 - (i/20)
		BottomAccent.BackgroundTransparency = 0 - (i/20)
		task.wait(0.02)
	end
end)

