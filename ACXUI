-- Animation Functions
local function animateHover(frame)
    local hover = false
    
    frame.MouseEnter:Connect(function()
        hover = true
        TweenService:Create(frame, TweenInfo.new(0.2), {
            BackgroundTransparency = 0.05
        }):Play()
        TweenService:Create(frame:FindFirstChildWhichIsA("UIStroke"), TweenInfo.new(0.2), {
            Transparency = 0.6
        }):Play()
    end)
    
    frame.MouseLeave:Connect(function()
        hover = false
        TweenService:Create(frame, TweenInfo.new(0.2), {
            BackgroundTransparency = 0.1
        }):Play()
        TweenService:Create(frame:FindFirstChildWhichIsA("UIStroke"), TweenInfo.new(0.2), {
            Transparency = 0.8
        }):Play()
    end)
    
    if frame:FindFirstChild("Check") then
        frame.MouseButton1Down:Connect(function()
            TweenService:Create(frame.Check, TweenInfo.new(0.1), {
                Size = UDim2.new(0.5, 0, 0.5, 0)
            }):Play()
        end)
        
        frame.MouseButton1Up:Connect(function()
            TweenService:Create(frame.Check, TweenInfo.new(0.1), {
                Size = UDim2.new(0.6, 0, 0.6, 0)
            }):Play()
        end)
    end
end

local function animateClick(frame)
    frame.MouseButton1Down:Connect(function()
        TweenService:Create(frame, TweenInfo.new(0.1), {
            Size = UDim2.new(0.98, 0, 0.96, 0)
        }):Play()
    end)
    
    frame.MouseButton1Up:Connect(function()
        TweenService:Create(frame, TweenInfo.new(0.1), {
            Size = UDim2.new(1, 0, 1, 0)
        }):Play()
    end)
end

-- SCRIPT

local function uiparent()
    local success, parent = pcall(function()
        return gethui()
    end)

    if not success then
        return game:GetService("CoreGui")
    end

    return parent
end

local success, err = pcall(function()
    Arceus.Parent = uiparent()
end)

if not success then
    Arceus.Parent = game:GetService("Players").LocalPlayer.PlayerGui
end

local element_height = 50
local elements = 0

local function AddSpace(parent)
    local space = tab:Clone()
    space.Parent = parent
    space.LayoutOrder = elements
    space.Visible = true
    elements += 1
end

-- UI Functions
function lib:AddToggle(name, funct, enabled, ...)
    local newTog = Toggle:Clone()
    local args = {...}
    local toggled = enabled
    
    newTog.Name = name
    newTog.Name.Text = name
    newTog.Check.Visible = toggled
    newTog.Visible = true
    newTog.LayoutOrder = elements
    newTog.Parent = Menu
    
    animateHover(newTog)
    animateClick(newTog)
    
    newTog.MouseButton1Click:Connect(function()
        toggled = not toggled
        TweenService:Create(newTog.Check, TweenInfo.new(0.2, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
            Size = toggled and UDim2.new(0.6, 0, 0.6, 0) or UDim2.new(0, 0, 0, 0)
        }):Play()
        newTog.Check.Visible = true
        funct(toggled, unpack(args))
    end)
    
    elements += 1
    AddSpace(Menu)
    
    return newTog
end

function lib:AddButton(name, funct, ...)
    local newBut = Button:Clone()
    local args = {...}
    
    newBut.Name = name
    newBut.Name_2.Text = name
    newBut.Visible = true
    newBut.LayoutOrder = elements
    newBut.Parent = Menu
    
    animateHover(newBut)
    animateClick(newBut)
    
    newBut.MouseButton1Click:Connect(function()
        TweenService:Create(newBut, TweenInfo.new(0.1), {
            BackgroundColor3 = COLOR_PRIMARY
        }):Play()
        task.wait(0.1)
        TweenService:Create(newBut, TweenInfo.new(0.3), {
            BackgroundColor3 = COLOR_CARD
        }):Play()
        funct(unpack(args))
    end)
    
    elements += 1
    AddSpace(Menu)
    
    return newBut
end

function lib:AddInputBox(name, funct, placeholder, default, ...)
    local newInput = InputBox:Clone()
    local args = {...}
    
    newInput.Name = name
    newInput.Name_5.Text = name
    newInput.TextBox.PlaceholderText = placeholder or "Enter text..."
    newInput.TextBox.Text = default or ""
    newInput.Visible = true
    newInput.LayoutOrder = elements
    newInput.Parent = Menu
    
    animateHover(newInput)
    
    local textBox = newInput.TextBox
    textBox.FocusLost:Connect(function(enterPressed)
        if enterPressed then
            funct(textBox.Text, unpack(args))
            TweenService:Create(textBox:FindFirstChildWhichIsA("UIStroke"), TweenInfo.new(0.2), {
                Color = COLOR_PRIMARY
            }):Play()
        end
    end)
    
    textBox.Focused:Connect(function()
        TweenService:Create(textBox:FindFirstChildWhichIsA("UIStroke"), TweenInfo.new(0.2), {
            Color = Color3.fromRGB(255, 255, 255),
            Transparency = 0.4
        }):Play()
    end)
    
    textBox.FocusLost:Connect(function()
        TweenService:Create(textBox:FindFirstChildWhichIsA("UIStroke"), TweenInfo.new(0.2), {
            Color = COLOR_PRIMARY,
            Transparency = 0.7
        }):Play()
    end)
    
    elements += 1
    AddSpace(Menu)
    
    local inputObj = {
        Frame = newInput,
        TextBox = textBox,
        GetText = function()
            return textBox.Text
        end,
        SetText = function(newText)
            textBox.Text = newText or ""
        end,
        Clear = function()
            textBox.Text = ""
        end,
        SetPlaceholder = function(placeholderText)
            textBox.PlaceholderText = placeholderText or "Enter text..."
        end
    }
    
    return inputObj
end

function lib:AddSlider(name, min, max, default, funct, ...)
    local newSlider = Slider:Clone()
    local args = {...}
    local value = default or min
    local dragging = false
    
    newSlider.Name = name
    newSlider.Name_6.Text = name .. ": " .. value
    newSlider.SliderValue.Text = tostring(value)
    newSlider.SliderFill.Size = UDim2.new((value - min) / (max - min), 0, 1, 0)
    newSlider.Visible = true
    newSlider.LayoutOrder = elements
    newSlider.Parent = Menu
    
    local function updateValue(newValue)
        value = math.clamp(newValue, min, max)
        newSlider.Name_6.Text = name .. ": " .. math.floor(value)
        newSlider.SliderValue.Text = tostring(math.floor(value))
        newSlider.SliderFill.Size = UDim2.new((value - min) / (max - min), 0, 1, 0)
        funct(value, unpack(args))
    end
    
    newSlider.SliderButton.MouseButton1Down:Connect(function()
        dragging = true
    end)
    
    game:GetService("UserInputService").InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = false
        end
    end)
    
    game:GetService("RunService").RenderStepped:Connect(function()
        if dragging then
            local mouse = game:GetService("Players").LocalPlayer:GetMouse()
            local track = newSlider.SliderTrack
            local trackPos = track.AbsolutePosition.X
            local trackSize = track.AbsoluteSize.X
            local mousePos = math.clamp(mouse.X - trackPos, 0, trackSize)
            local percent = mousePos / trackSize
            updateValue(min + (max - min) * percent)
        end
    end)
    
    elements += 1
    AddSpace(Menu)
    
    return newSlider
end

function lib:AddComboBox(text, options, funct, ...)
    local newCombo = ComboBox:Clone()
    local enabled = false
    local elems = {}
    local args = {...}
    local selected = options[1] or "Select"
    
    newCombo.Name = text
    newCombo.Name_4.Text = text .. ": " .. selected
    newCombo.Visible = true
    newCombo.LayoutOrder = elements
    newCombo.Parent = Menu
    
    animateHover(newCombo)
    animateClick(newCombo)
    
    local function setBoxState()
        TweenService:Create(newCombo.Img_2, TweenInfo.new(0.3), {
            Rotation = enabled and 0 or 180
        }):Play()
        
        for _, elem in ipairs(elems) do
            if enabled then
                elem.Visible = true
                TweenService:Create(elem, TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
                    Position = UDim2.new(0, 0, 0, 0)
                }):Play()
            else
                TweenService:Create(elem, TweenInfo.new(0.2), {
                    Position = UDim2.new(0, 0, -0.5, 0)
                }):Play()
                task.wait(0.2)
                elem.Visible = false
            end
        end
    end
    
    newCombo.MouseButton1Click:Connect(function()
        enabled = not enabled
        setBoxState()
    end)
    
    elements += 1
    AddSpace(Menu)
    
    for _, name in ipairs(options) do
        local newElem = ComboElem:Clone()
        table.insert(elems, newElem)
        
        newElem.Name = name
        newElem.Name_3.Text = name
        newElem.Visible = false
        newElem.Position = UDim2.new(0, 0, -0.5, 0)
        newElem.LayoutOrder = elements
        newElem.Parent = Menu
        
        animateHover(newElem)
        animateClick(newElem)
        
        newElem.MouseButton1Click:Connect(function()
            selected = name
            newCombo.Name_4.Text = text .. ": " .. selected
            enabled = false
            setBoxState()
            funct(name, unpack(args))
            
            TweenService:Create(newElem, TweenInfo.new(0.2), {
                BackgroundColor3 = COLOR_PRIMARY
            }):Play()
            task.wait(0.2)
            TweenService:Create(newElem, TweenInfo.new(0.3), {
                BackgroundColor3 = COLOR_CARD
            }):Play()
        end)
        
        elements += 1
        AddSpace(Menu)
    end
    
    return newCombo
end

function lib:SetTitle(txt)
    Title.Text = txt
    TitleGlow.Text = txt
end

function lib:SetIcon(img)
    Logo.Image = img
end

function lib:SetBackgroundColor(r, g, b)
    Main.BackgroundColor3 = Color3.fromRGB(r, g, b)
    Intro.BackgroundColor3 = Color3.fromRGB(r, g, b)
end

function lib:SetTitleColor(r, g, b)
    Title.TextColor3 = Color3.fromRGB(r, g, b)
end

function lib:SetCloseBtnColor(r, g, b)
    Close.ImageColor3 = Color3.fromRGB(r, g, b)
    CloseGlow.ImageColor3 = Color3.fromRGB(r, g, b)
end

function lib:SetButtonsColor(r, g, b)
    Toggle.Check.BackgroundColor3 = Color3.fromRGB(r, g, b)
    Button.BackgroundColor3 = Color3.fromRGB(r, g, b)
    ComboElem.Img.ImageColor3 = Color3.fromRGB(r, g, b)
    ComboBox.Img_2.ImageColor3 = Color3.fromRGB(r, g, b)
    InputBox.TextBoxStroke.Color = Color3.fromRGB(r, g, b)
    Slider.SliderFill.BackgroundColor3 = Color3.fromRGB(r, g, b)
end

function lib:SetTheme(theme)
    if theme == "Default" then
        -- Already set
    elseif theme == "TomorrowNightBlue" then
        lib:SetButtonsColor(74, 208, 238)
        lib:SetCloseBtnColor(74, 208, 238)
        lib:SetBackgroundColor(5, 16, 58)
    elseif theme == "HighContrast" then
        lib:SetBackgroundColor(0, 0, 0)
        lib:SetButtonsColor(0, 0, 0)
        lib:SetCloseBtnColor(255, 255, 0)
    elseif theme == "Aqua" then
        lib:SetBackgroundColor(44, 62, 82)
        lib:SetButtonsColor(52, 74, 95)
        lib:SetCloseBtnColor(26, 189, 158)
    elseif theme == "Ocean" then
        lib:SetBackgroundColor(26, 32, 58)
        lib:SetButtonsColor(38, 45, 71)
        lib:SetCloseBtnColor(86, 76, 251)
    else
        error("Theme not found.")
    end
end

-- Close button behavior
Close.MouseButton1Click:Connect(function()
    Logo.Active = true
    
    TweenService:Create(Intro, TweenInfo.new(0.3), {BackgroundTransparency = 0}):Play()
    TweenService:Create(Main, TweenInfo.new(0.3), {BackgroundTransparency = 0.3}):Play()
    
    task.wait(0.3)
    
    -- Hide all children except Intro
    for _, obj in pairs(Main:GetChildren()) do
        if obj:IsA("GuiObject") and obj ~= Intro then
            TweenService:Create(obj, TweenInfo.new(0.2), {
                Position = obj.Position + UDim2.new(0, 0, 0.5, 0),
                BackgroundTransparency = 1
            }):Play()
        end
    end
    
    task.wait(0.2)
    
    Main:TweenSize(
        UDim2.fromScale(0.15, 0.15),
        Enum.EasingDirection.Out,
        Enum.EasingStyle.Quad,
        0.3, true, nil
    )
    
    TweenService:Create(LogoGlow, TweenInfo.new(0.3), {ImageTransparency = 0.5}):Play()
    TweenService:Create(Intro, TweenInfo.new(0.3), {BackgroundTransparency = 1}):Play()
end)

-- Logo click behavior
Logo.MouseButton1Click:Connect(function()
    Logo.Active = false
    
    TweenService:Create(LogoGlow, TweenInfo.new(0.3), {ImageTransparency = 0.7}):Play()
    TweenService:Create(Intro, TweenInfo.new(0.3), {BackgroundTransparency = 0}):Play()
    TweenService:Create(Main, TweenInfo.new(0.3), {BackgroundTransparency = 0.15}):Play()
    
    Main:TweenSize(
        UDim2.fromOffset(400, 500),
        Enum.EasingDirection.Out,
        Enum.EasingStyle.Back,
        0.5, true, nil
    )
    
    task.wait(0.5)
    
    -- Show all children with animation
    for _, obj in pairs(Main:GetChildren()) do
        if obj:IsA("GuiObject") and obj ~= Intro then
            obj.Position = obj.Position - UDim2.new(0, 0, 0.5, 0)
            TweenService:Create(obj, TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
                Position = obj.Position + UDim2.new(0, 0, 0.5, 0),
                BackgroundTransparency = obj.BackgroundTransparency
            }):Play()
        end
    end
    
    task.wait(0.3)
    TweenService:Create(Intro, TweenInfo.new(0.3), {BackgroundTransparency = 1}):Play()
end)

-- Initial animation
Main:TweenPosition(
    UDim2.fromScale(0.5, 0.5),
    Enum.EasingDirection.Out,
    Enum.EasingStyle.Back,
    1, true, nil
)

task.wait(1)
TweenService:Create(LogoGlow, TweenInfo.new(0.5), {ImageTransparency = 0.7}):Play()
task.wait(0.5)
TweenService:Create(Intro, TweenInfo.new(0.5), {BackgroundTransparency = 1}):Play()

return lib
